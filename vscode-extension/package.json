{
  "name": "markasset",
  "displayName": "MarkAsset",
  "repository": {
    "type": "git",
    "url": "git://github.com/username/repository.git"
  },
  "description": "Streamline adding images to markdown documents with upload codes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icons/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markasset.startSession",
        "title": "Start Upload Session",
        "category": "MarkAsset"
      }
    ],
    "configuration": {
      "title": "MarkAsset",
      "properties": {
        "markasset.firebaseProjectId": {
          "type": "string",
          "default": "markasset-project",
          "description": "Firebase Project ID"
        },
        "markasset.firebaseApiKey": {
          "type": "string",
          "default": "",
          "description": "Firebase API Key"
        },
        "markasset.pollInterval": {
          "type": "number",
          "default": 2000,
          "description": "Poll interval in milliseconds for checking uploaded files"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "firebase": "^10.7.1"
  }
}
